{
  "name": "@microrealestate/tenant",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean:types": "rimraf ../../types/dist",
    "transpile:types": "tsc --build ../../types/tsconfig.json",
    "transpile:app": "next build --no-lint",
    "watch:types": "tsc --build ../../types/tsconfig.json --watch --preserveWatchOutput",
    "watch:app": "next dev",
    "dev": "npm-run-all clean:types transpile:types --parallel watch:types watch:app",
    "prebuild": "node scripts/generate-version.js",
    "build": "npm-run-all clean:types transpile:types transpile:app",
    "generate-version": "node scripts/generate-version.js",
    "start": "next start",
    "lint": "next lint --no-cache",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json,css}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:cross-browser": "node scripts/cross-browser-test.js",
    "test:cross-browser:verbose": "node scripts/cross-browser-test.js --verbose",
    "analyze-bundle": "node scripts/analyze-bundle.js",
    "bundle-size": "npm run build && npm run analyze-bundle"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "0.4.2",
    "@hookform/resolvers": "3.3.2",
    "@radix-ui/react-avatar": "1.1.3",
    "@radix-ui/react-dialog": "1.1.6",
    "@radix-ui/react-hover-card": "1.1.6",
    "@radix-ui/react-label": "2.1.2",
    "@radix-ui/react-popover": "1.1.6",
    "@radix-ui/react-select": "2.1.6",
    "@radix-ui/react-separator": "1.1.2",
    "@radix-ui/react-slot": "1.1.2",
    "@radix-ui/react-toast": "1.2.6",
    "@radix-ui/react-tooltip": "1.1.8",
    "axios": "1.8.4",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cmdk": "1.0.4",
    "date-fns": "2.30.0",
    "input-otp": "1.4.2",
    "js-file-download": "0.4.12",
    "libphonenumber-js": "1.11.14",
    "lucide-react": "0.475.0",
    "moment": "2.30.1",
    "negotiator": "0.6.3",
    "next": "14.2.26",
    "next-runtime-env": "3.2.1",
    "next-themes": "0.3.0",
    "react": "18.2.0",
    "react-day-picker": "8.10.1",
    "react-dom": "18.2.0",
    "react-hook-form": "7.54.2",
    "usehooks-ts": "3.1.0",
    "vaul": "1.1.2",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@happy-dom/jest-environment": "^18.0.1",
    "@microrealestate/types": "*",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.12",
    "@types/negotiator": "0.6.2",
    "@types/node": "22.13.5",
    "@types/react": "18.2.32",
    "@types/react-dom": "18.2.14",
    "@typescript-eslint/eslint-plugin": "7.15.0",
    "@typescript-eslint/parser": "7.15.0",
    "autoprefixer": "10.4.20",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.26",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.5.3",
    "rimraf": "6.0.1",
    "tailwind-merge": "2.5.2",
    "tailwindcss": "3.4.10",
    "tailwindcss-animate": "1.0.7",
    "ts-jest": "^29.1.2",
    "typescript": "5.7.3"
  }
}
